    <script>
        window.onload = defaultMap;

        function defaultMap() {
            document.getElementById("heatMapTab").classList.add("exp_tab_active");
        }

        function displayMap(id) {
            //remove all tab class
            var tabs = document.getElementsByClassName("exp_tab");
            for (var i = 0; i < tabs.length; i++) {
                tabs[i].classList.remove("exp_tab_active");
            }

            //add active to id
            document.getElementById(id + "Tab").classList.add("exp_tab_active");

            //display tableau
            document.getElementById("heatMap").style.display = "none";
            document.getElementById("Statistics").style.display = "none";
            document.getElementById("bubbleMap").style.display = "none";
            document.getElementById(id).style.display = "block";
        }
    </script>
